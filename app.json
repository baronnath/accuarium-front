{
  "expo": {
    "name": "accuarium",
    "slug": "front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan"
    }
  },
  "navigator": {
    "screenOptions": {
      "headerShown": false
    }
  },
  "backend": {
    "url": "http://192.168.0.19:8080",
    "imagesUrl": "http://192.168.0.19:8080/images/"
  },
  "preferences": {
    "pagination": 10
  }
}
